<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.myapp" version="1.0.0"
        xmlns="http://www.w3.org/ns/widgets">
    <name>Hello</name>
    <description>
        A sample Weex application.
    </description>
    <author email="" href="">
        Weex Team
    </author>
    <content src="index.html"/>
    <preference name="loglevel" value="DEBUG"/>
    <preference name="launch_locally" value="true"/>
    <preference name="launch_url" value="http://127.0.0.1:8080/dist/index.js"/>
    <preference name="local_url" value="file://assets/index.js"/>

    <feature name="richtext">
        <param name="api" value="richtext"/>
        <param name="android-package" value="com.alibaba.weex.extend.component.RichText"/>
        <param name="category" value="component"/>
    </feature>
    <feature name="render">
        <param name="api" value="render"/>
        <param name="android-package" value="com.alibaba.weex.extend.module.RenderModule"/>
        <param name="category" value="module"/>
    </feature>
    <feature name="event">
        <param name="api" value="event"/>
        <param name="android-package" value="com.alibaba.weex.extend.module.WXEventModule"/>
        <param name="category" value="module"/>
    </feature>
    <feature name="myModule">
        <param name="api" value="myModule"/>
        <param name="android-package" value="com.alibaba.weex.extend.module.MyModule"/>
        <param name="category" value="module"/>
    </feature>
    <feature name="device">
        <param name="api" value="device"/>
        <param name="android-package" value="com.alibaba.weex.extend.module.DeviceModule"/>
        <param name="category" value="module"/>
    </feature>
</widget>
